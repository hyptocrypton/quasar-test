#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yannik/git/vera-frontend/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/node_modules:/Users/yannik/git/vera-frontend/node_modules:/Users/yannik/git/node_modules:/Users/yannik/node_modules:/Users/node_modules:/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/cache-loader@4.1.0_webpack@5.66.0/node_modules/cache-loader/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/cache-loader@4.1.0_webpack@5.66.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yannik/git/vera-frontend/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/node_modules:/Users/yannik/git/vera-frontend/node_modules:/Users/yannik/git/node_modules:/Users/yannik/node_modules:/Users/node_modules:/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/cache-loader@4.1.0_webpack@5.66.0/node_modules/cache-loader/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/cache-loader@4.1.0_webpack@5.66.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
fi
