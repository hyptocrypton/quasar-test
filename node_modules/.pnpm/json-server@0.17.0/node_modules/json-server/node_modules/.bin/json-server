#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yannik/git/vera-frontend/node_modules/.pnpm/json-server@0.17.0/node_modules/json-server/lib/cli/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/json-server@0.17.0/node_modules/json-server/lib/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/json-server@0.17.0/node_modules/json-server/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/json-server@0.17.0/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/node_modules:/Users/yannik/git/vera-frontend/node_modules:/Users/yannik/git/node_modules:/Users/yannik/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yannik/git/vera-frontend/node_modules/.pnpm/json-server@0.17.0/node_modules/json-server/lib/cli/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/json-server@0.17.0/node_modules/json-server/lib/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/json-server@0.17.0/node_modules/json-server/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/json-server@0.17.0/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/node_modules:/Users/yannik/git/vera-frontend/node_modules:/Users/yannik/git/node_modules:/Users/yannik/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli/bin.js" "$@"
else
  exec node  "$basedir/../../lib/cli/bin.js" "$@"
fi
