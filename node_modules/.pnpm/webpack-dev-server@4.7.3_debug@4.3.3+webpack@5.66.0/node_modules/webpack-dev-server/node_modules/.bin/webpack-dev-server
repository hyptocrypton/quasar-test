#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yannik/git/vera-frontend/node_modules/.pnpm/webpack-dev-server@4.7.3_debug@4.3.3+webpack@5.66.0/node_modules/webpack-dev-server/bin/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/webpack-dev-server@4.7.3_debug@4.3.3+webpack@5.66.0/node_modules/webpack-dev-server/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/webpack-dev-server@4.7.3_debug@4.3.3+webpack@5.66.0/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/node_modules:/Users/yannik/git/vera-frontend/node_modules:/Users/yannik/git/node_modules:/Users/yannik/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yannik/git/vera-frontend/node_modules/.pnpm/webpack-dev-server@4.7.3_debug@4.3.3+webpack@5.66.0/node_modules/webpack-dev-server/bin/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/webpack-dev-server@4.7.3_debug@4.3.3+webpack@5.66.0/node_modules/webpack-dev-server/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/webpack-dev-server@4.7.3_debug@4.3.3+webpack@5.66.0/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/node_modules:/Users/yannik/git/vera-frontend/node_modules:/Users/yannik/git/node_modules:/Users/yannik/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../bin/webpack-dev-server.js" "$@"
fi
