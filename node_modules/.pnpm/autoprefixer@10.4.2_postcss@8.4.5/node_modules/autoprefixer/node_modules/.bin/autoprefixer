#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yannik/git/vera-frontend/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules/autoprefixer/bin/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules/autoprefixer/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/node_modules:/Users/yannik/git/vera-frontend/node_modules:/Users/yannik/git/node_modules:/Users/yannik/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yannik/git/vera-frontend/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules/autoprefixer/bin/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules/autoprefixer/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules:/Users/yannik/git/vera-frontend/node_modules/.pnpm/node_modules:/Users/yannik/git/vera-frontend/node_modules:/Users/yannik/git/node_modules:/Users/yannik/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/autoprefixer" "$@"
else
  exec node  "$basedir/../../bin/autoprefixer" "$@"
fi
